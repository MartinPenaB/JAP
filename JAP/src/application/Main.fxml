<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="mainAnchorPane" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.MainController">
	<children>
		<AnchorPane prefHeight="205.0" prefWidth="415.0">
			<children>
				<Button layoutX="95.0" layoutY="155.0" mnemonicParsing="false" onAction="#goToProject" prefHeight="35.0" prefWidth="65.0" text="OK" />
				<Button layoutX="175.0" layoutY="155.0" mnemonicParsing="false" onAction="#quitApplication" prefHeight="35.0" prefWidth="65.0" text="Close" />
				<Button layoutX="255.0" layoutY="155.0" mnemonicParsing="false" onAction="#openHelp" prefHeight="35.0" prefWidth="65.0" text="Help" />
				<ComboBox fx:id="projectComboBox" layoutX="185.0" layoutY="70.0" prefHeight="25.0" prefWidth="205.0" promptText="Select Project" />
				<ImageView fitHeight="85.0" fitWidth="135.0" layoutX="30.0" layoutY="50.0" pickOnBounds="true" preserveRatio="true">
					<image>
						<Image url="@../CSmin.png" />
					</image>
				</ImageView>
			</children>
		</AnchorPane>
	</children>
</AnchorPane>
